<style>
    /* Always set the map height explicitly to define the size of the div
    * element that contains the map. */
    #map {
        height: 500px;
    }
</style>
<div class="container">
    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -34.397, lng: 150.644},
          zoom: 8
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3&key=AIzaSyCcz0tiTXcyMzZDZv2eVS_3QgCy34hEPXw&callback=initMap"
    async defer></script>
</div>
